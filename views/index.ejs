<%- include("./partials/header.ejs") %>
    <div class="container">
        <div class="stopwatch">
            <div class = "stpwtc">
                <h2 class="stopwatch-heading">Time since you started studying</h2>
                <div class="stopwatch-inner">
                    <div id="stopwatch-display">00:00:00</div>
                    <div class="button-group">
                        <button id="start-btn">Start</button>
                        <button id="pause-btn">Pause</button>
                        <button id="reset-btn">Reset</button>
                    </div>
                </div>
            </div>
        </div>
        <div class="flow-state" id="flow-state-msg">
            
        </div>

        <div class="get-quote">
            <h2><%= quote%></h2>
            <p>&emsp;&emsp;&emsp; -<%= author%></p>
        </div>
        <div class="to-do-list">

        </div>

    </div>
<%- include("./partials/footer.ejs") %>